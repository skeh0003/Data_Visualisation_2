{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": { "text": "Victorian Industries" },
    "width": 350,
    "height": 500,
    "data": {
      "url": "./State_Industries.csv"
    },
    "mark": "bar",
    "encoding": {
      "x": { "field": "Victoria", "type": "quantitative", "title": "Employment (Thousands)"},
      "y": { "field": "Industry", "type": "nominal" },
      "color": {
        "condition": {
          "test": "datum.Industry === 'Retail Trade'",
          "value": "green"
        },
        "value": "steelblue"
      },
      "tooltip": [
        { "field": "Industry", "type": "nominal", "title": "Industry" },
        { "field": "Victoria", "type": "quantitative", "title": "Employment (Thousands)" }
      ]
    }
  }
  